  "homepage": "https://github.com/CodeXblock/vite-plugin-svelte-xblock#readme",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^5.3.1"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
